<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="16dp">


    <EditText
        android:id="@+id/txtCompare"
        android:layout_width="384dp"
        android:layout_height="61dp"
        android:hint="Syötä kunnan nimi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.657"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnCompare"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Vertaile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtCompare" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout1"
        android:layout_width="429dp"
        android:layout_height="246dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCompare">

        <TextView
            android:id="@+id/txtPopulationCcompare1"
            android:layout_width="142.5dp"
            android:layout_height="match_parent"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="parent"
            tools:layout_editor_absoluteY="1dp" />

        <TextView
            android:id="@+id/txtPopulationCompare1"
            android:layout_width="142.5dp"
            android:layout_height="match_parent"
            android:layout_marginStart="4dp"
            android:text="TextView"
            app:layout_constraintEnd_toStartOf="@+id/txtPopulationCcompare1"

            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/txtWeatherCompare1"
            tools:layout_editor_absoluteY="28dp" />

        <TextView
            android:id="@+id/txtWeatherCompare1"
            android:layout_width="142.5dp"
            android:layout_height="match_parent"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="429dp"
        android:layout_height="246dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout1">

        <TextView
            android:id="@+id/txtPopulationCcompare2"
            android:layout_width="142.5dp"
            android:layout_height="match_parent"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="parent"
            tools:layout_editor_absoluteY="1dp" />

        <TextView
            android:id="@+id/txtPopulationCompare2"
            android:layout_width="142.5dp"
            android:layout_height="match_parent"
            android:layout_marginStart="4dp"
            android:text="TextView"
            app:layout_constraintEnd_toStartOf="@+id/txtPopulationCcompare2"

            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/txtWeatherCompare2"
            tools:layout_editor_absoluteY="28dp" />

        <TextView
            android:id="@+id/txtWeatherCompare2"
            android:layout_width="142.5dp"
            android:layout_height="match_parent"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
